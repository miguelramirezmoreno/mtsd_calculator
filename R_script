install.packages("remotes")
remotes::install_github("GRousselet/bootcorci")
library(bootcorci)
Data <- read.csv("/Users/username/Desktop/R_mtsd/Fig5all.csv")
x1 <-c(na.omit(Data[,1]))
y1 <-c(na.omit(Data[,2]))
x2 <-c(na.omit(Data[,3]))
y2 <-c(na.omit(Data[,4]))

res <- twocorci(x1, y1, x2, y2, method = "pearson", alternative = "two.sided")
